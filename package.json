{
	"name": "Nexter",
	"version": "1.0.0",
	"description": "Landing page for Nexter",
	"main": "index.js",
	"scripts": {
		"start": "sass sass/main.scss css/style.css -w",
		"scss": "sass sass/main.scss css/style.comp.css",
		"css-p": "postcss css/style.comp.css --use autoprefixer -d css/",
		"css": "sass css/style.comp.css css/style.css --style compressed",
		"build": "npm-run-all scss css-p css"
	},
	"author": "Vatsal",
	"license": "ISC",
	"devDependencies": {
		"autoprefixer": "^10.3.5",
		"npm-run-all": "^4.1.5",
		"postcss-cli": "^8.3.1",
		"sass": "^1.42.1"
	}
}
